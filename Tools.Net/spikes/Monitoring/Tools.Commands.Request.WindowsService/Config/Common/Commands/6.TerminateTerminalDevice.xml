    <constructor-arg index="0" value="${GetCommandsSPName}"/>
    <constructor-arg index="1">
      <dictionary key-type="decimal" value-type="ICommandExecutor">
        <entry key="6" value-ref="TerminateTerminalDeviceCommandExecutor" />
